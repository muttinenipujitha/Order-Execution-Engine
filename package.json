{
  "name": "order-execution-engine-prisma",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n API,WORKER -c auto \"tsx watch src/server.ts\" \"tsx watch src/worker.ts\"",
    "api": "tsx watch src/server.ts",
    "worker": "tsx watch src/worker.ts",
    "build": "tsc -p tsconfig.json && prisma generate",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "db:migrate": "prisma migrate dev --name init",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "@fastify/websocket": "^8.3.1",
    "@prisma/client": "^6.1.0",
    "bullmq": "^5.20.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.29.1",
    "ioredis": "^5.4.1",
    "pino-pretty": "^11.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.12",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "ws": "^8.18.0"
  }
}
